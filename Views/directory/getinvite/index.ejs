<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>Xitysites</title>
    <%- include('../../layout/css'); %>
  </head>

  <body>
    <section class="main">
      <div class="container-fluid p-0">
        <!-- start admin dashboard-section -->
        <section class="admin-dashboard">
          <div class="dashboard p-0">
            <div class="d-flex" id="addFlex">
              <!-- start sidebar section  -->
              <%- include('../../layout/sidebar'); %>

              <!-- end sidebar section  -->

              <!-- start content section  -->
              <div class="sec-2">
                <div class="tab-content" id="v-pills-tabContent">
                  <%- include('../../layout/header'); %>

                  <!-- start breadcrumb section  -->
                  <section class="breadcrumb-section">
                    <div class="row">
                      <div class="col-md-6">
                        <div class="bread-sec1">
                          <h6 class="title">Get Invite</h6>
                          <p class="sub-cont">
                            Lorem Ipsum is simply dummy text of the printing
                          </p>
                        </div>
                      </div>
                      <div class="col-md-6">
                        <div class="bread-sec2">
                          <nav aria-label="breadcrumb">
                            <ol class="breadcrumb align-bread">
                              <li class="breadcrumb-item">
                                <a href="#"
                                  ><img
                                    class="bread-home"
                                    src="/assets2/img/icons/bread-home.png"
                                    alt=""
                                /></a>
                              </li>
                              <li class="breadcrumb-item">
                                <a class="bread-menu" href="#">Get Invite</a>
                              </li>
                              <li class="breadcrumb-item" aria-current="page">
                                <a class="bread-menu" href="#">Get Invite</a>
                              </li>
                            </ol>
                          </nav>
                        </div>
                      </div>
                    </div>
                  </section>
                  <!-- end breadcrumb section  -->

                  <!-- start main-content section  -->
                  <section class="main-content">
                    <div class="row row-padd">
                      <div class="col-lg-8">
                        <div class="listing-div">
                          <h6 class="list-title">All Listing Get Invite</h6>
                          <p class="list-subtext">
                            Lorem ipsum dolor sit amet, consectetur adipiscing
                            elit, sed do eiusmod tempor incididunt
                          </p>
                        </div>
                      </div>
                      <!-- <%- include('./dropdown'); %> -->
                    </div>

                    <div class="table-section">
                      <table class="table">
                        <thead>
                          <tr>
                            <td>S.No</td>
                            <td>name</td>
                            <td>email</td>
                            <td>phoneno</td>
                            <td>Message</td>
                          </tr>
                        </thead>
                        <tbody id="tbody">
                          <% var sno=1 %> <% data.forEach(function(category) {
                          %>
                          <tr>
                            <td><%= sno++ %></td>
                            <td>
                              <h6 class="lis-name"><%= category.name %></h6>
                            </td>
                            <td>
                              <h6 class="lis-name"><%= category.email %></h6>
                            </td>
                            <td>
                              <h6 class="lis-name"><%= category.phoneno %></h6>
                            </td>
                            <td>
                              <h6 class="lis-name"><%= category.message %></h6>
                            </td>
                          </tr>
                          <% }); %>
                        </tbody>
                      </table>
                    </div>
                  </section>
                  <!-- end main-content section  -->
                </div>
              </div>
              <!-- end content section  -->
            </div>
          </div>
        </section>
        <!-- end admin dashboard-section -->
      </div>
    </section>
    <%- include('../../layout/fotermain'); %>

    <script src="https://unpkg.com/axios/dist/axios.min.js"></script>
  </body>
</html>
