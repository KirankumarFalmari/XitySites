    <script>
      function getdata(event){
      //  event.preventDefault()  
      const name = document.getElementById("selectcountry").value;
      // console.log();
      
      var htmldata="";
       axios.get("http://test.profcymaglobal.com/master/city/data")
       .then(function(res){
      
        res.data.forEach(function(data){
         
          if(name===data.countryid){
          htmldata+=`
          <option value="${data._id}">${data.name}</option>
          `;
          }
        })
        document.getElementById("selectstate").innerHTML=htmldata;
      })
      }
        </script>
    
