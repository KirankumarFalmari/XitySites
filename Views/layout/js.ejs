<script>
  $(window).on("load", function () {
    var alterClass = function () {
      var width = document.body.clientWidth;
      if (width < 769) {
        $("#offcanvasScrolling").addClass("offcanvas offcanvas-start");
      } else if (width >= 768) {
        $("#offcanvasScrolling").removeClass("offcanvas offcanvas-start");
      }
    };

    $(window).resize(function () {
      alterClass();
    });
    alterClass();
  });
</script>

