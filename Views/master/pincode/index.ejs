<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>Xitysites</title>
    <%- include('../../layout/css'); %>
  </head>

  <body>
    <section class="main">
      <div class="container-fluid p-0">
        <!-- start admin dashboard-section -->
        <section class="admin-dashboard">
          <div class="dashboard p-0">
            <div class="d-flex" id="addFlex">
              <!-- start sidebar section  -->
              <%- include('../../layout/sidebar'); %>

              <!-- end sidebar section  -->

              <!-- start content section  -->
              <div class="sec-2">
                <div class="tab-content" id="v-pills-tabContent">
                  <%- include('../../layout/header'); %>

                  <!-- start breadcrumb section  -->
                  <section class="breadcrumb-section">
                    <div class="row">
                      <div class="col-md-6">
                        <div class="bread-sec1">
                          <h6 class="title">Sample Text</h6>
                          <p class="sub-cont">
                            Lorem Ipsum is simply dummy text of the printing
                          </p>
                        </div>
                      </div>
                      <div class="col-md-6">
                        <div class="bread-sec2">
                          <nav aria-label="breadcrumb">
                            <ol class="breadcrumb align-bread">
                              <li class="breadcrumb-item">
                                <a href="#"
                                  ><img
                                    class="bread-home"
                                    src="assets2/img/icons/bread-home.png"
                                    alt=""
                                /></a>
                              </li>
                              <li class="breadcrumb-item">
                                <a class="bread-menu" href="#">Pages</a>
                              </li>
                              <li class="breadcrumb-item" aria-current="page">
                                <a class="bread-menu" href="#">Sample Page</a>
                              </li>
                            </ol>
                          </nav>
                        </div>
                      </div>
                    </div>
                  </section>
                  <!-- end breadcrumb section  -->

                  <!-- start main-content section  -->
                  <section class="main-content">
                    <div class="row row-padd">
                      <div class="col-lg-8">
                        <div class="listing-div">
                          <h6 class="list-title">All Listing Categories</h6>
                          <p class="list-subtext">
                            Lorem ipsum dolor sit amet, consectetur adipiscing
                            elit, sed do eiusmod tempor incididunt
                          </p>
                        </div>
                      </div>
                      <%- include('./dropdown'); %>
                    </div>

                    <div class="table-section">
                      <table class="table">
                        <thead>
                          <tr>
                            <td>S.No</td>
                            <td>Listing Country</td>
                            <td>Listing State</td>
                            <td>Listing City</td>
                            <td>Listing Town</td>
                            <td>Listing Pincode</td>
                            <td>Date</td>
                            <td>Edit</td>
                            <td>Delete</td>
                          </tr>
                        </thead>
                        <tbody id="tbody">
                          <% var sno=1 %> <% pdata.forEach(function(pincode) {
                          %>
                          <tr>
                            <td><%= sno++ %></td>
                            <% cdata.forEach(function(country){
                            if(country._id==pincode.countryid){ %>
                            <td>
                              <h6 class="lis-name"><%= country.name %></h6>
                            </td>
                            <%} })%> <% sdata.forEach(function(state){
                            if(state._id==pincode.stateid &&
                            state.countryid==pincode.countryid ){ %>
                            <td>
                              <h6 class="lis-name"><%= state.name %></h6>
                            </td>
                            <%} })%> <% data.forEach(function(city){
                            if(city._id==pincode.cityid &&
                            city.countryid==pincode.countryid &&
                            city.stateid==pincode.stateid ){ %>
                            <td>
                              <h6 class="lis-name"><%= city.name %></h6>
                            </td>
                            <%} })%>
                            <td>
                              <h6 class="lis-name"><%= pincode.name %></h6>
                            </td>

                            <td>
                              <button class="listing-btn">
                                <%= moment(pincode.created).format(
                                'MMM-DD-YYYY') %>
                              </button>
                            </td>

                            <td>
                              <h6 class="lis-name"><%= pincode.pincodeno %></h6>
                            </td>

                            <td>
                              <a
                                href="https://xitysites.onrender.com/master/pincode/edit/<%= pincode._id%>"
                              >
                                <img
                                  class="icon-img"
                                  src="assets/images/icon/edit.png"
                                  alt=""
                                />
                              </a>
                            </td>
                            <td>
                              <a
                                href="https://xitysites.onrender.com/master/pincode/delete/<%= pincode._id%>"
                              >
                                <img
                                  class="icon-img"
                                  src="assets/images/icon/delete.png"
                                  alt=""
                                />
                              </a>
                            </td>
                          </tr>
                          <% }); %>
                        </tbody>
                      </table>
                    </div>
                  </section>
                  <!-- end main-content section  -->
                </div>
              </div>
              <!-- end content section  -->
            </div>
          </div>
        </section>
        <!-- end admin dashboard-section -->
      </div>
    </section>
    <%- include('../../layout/fotermain'); %>
  </body>
</html>
