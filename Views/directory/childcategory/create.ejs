                          <script>
                            function getdata(event){
                            //  event.preventDefault()  
                            const name = document.getElementById("selectcategory").value;
                            // console.log();
                            
                            var htmldata="";
                             axios.get("http://localhost:5000/directory/childcategory/data")
                             .then(function(res){
                            
                              res.data.forEach(function(data){
                               
                                if(name===data.categoryid){
                                htmldata+=`
                                <option value="${data._id}">${data.name}</option>
                                `;
                                }
                              })
                              document.getElementById("selectsubcategory").innerHTML=htmldata;
                            })
                            }
                              </script>
                          
                          <script src="https://unpkg.com/axios/dist/axios.min.js"></script>
                         
                       
                        
