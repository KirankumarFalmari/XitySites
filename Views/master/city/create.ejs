    <section class="main">
      <div class="container-fluid p-0">
        <!-- start admin dashboard-section -->
        <section class="admin-dashboard">
          <div class="dashboard p-0">
            <div class="d-flex" id="addFlex">
              <!-- start sidebar section  -->
              <%- include('../../layout/sidebar'); %>

              <!-- end sidebar section  -->

              <!-- start content section  -->
              <div class="sec-2">
                <div class="tab-content" id="v-pills-tabContent">
                  <%- include('../../layout/header'); %>

                  <!-- start breadcrumb section  -->
                  <section class="breadcrumb-section">
                    <div class="row">
                      <div class="col-md-6">
                        <div class="bread-sec1">
                          <h6 class="title">Add Childcategory</h6>
                          <p class="sub-cont">
                            <!-- Lorem Ipsum is simply dummy text of the printing -->
                          </p>
                        </div>
                      </div>
                      <div class="col-md-6">
                        <div class="bread-sec2">
                          <nav aria-label="breadcrumb">
                            <ol class="breadcrumb align-bread">
                              <li class="breadcrumb-item">
                                <a href="#"
                                  ><img
                                    class="bread-home"
                                    src="assets2/img/icons/bread-home.png"
                                    alt=""
                                /></a>
                              </li>
                              <li class="breadcrumb-item">
                                <a
                                  class="bread-menu"
                                  href="http://xitysiteadmin.profcymadev.in:5000/master/city"
                                  >City List</a
                                >
                              </li>
                              <li class="breadcrumb-item" aria-current="page">
                                <a class="bread-menu" href="#">Add City</a>
                              </li>
                            </ol>
                          </nav>
                        </div>
                      </div>
                    </div>
                  </section>
                  <section class="main-content">
                  
                    <div class="table-section">
                      <form method="post" action="http://xitysiteadmin.profcymadev.in:5000/master/city/create" >
                        <div class="row">
                          <div class="col-md-6 px-5">
                          <div class="mb-4 row">
                            <label for="unit" class="col-md-2 col-sm-4 form-label">select Country</label>
                            <div class="col-md-8 col-sm-8">
                              
                             <select class="form-select" id="selectcountry" name="selectcountry" onfocusout="getdata(event)">
                              <option value="">Select country</option>
                                  <% cdata.forEach(function(country){ %>
                                   <option value="<%= country._id %>"><%= country.name %></option>
                                    <% }) %>
                             </select>
                         
                              <div class="form-text text-muted">select country</div>
                            </div>
                          </div>
                          <div class="mb-4 row">
                            <label for="unit" class="col-md-2 col-sm-4 form-label">select state</label>
                            <div class="col-md-8 col-sm-8">
                              
                             <select class="form-select" id="selectstate" name="selectstate">
                              <option value="">Select state</option>
                                  <!-- <% sdata.forEach(function(category){ %>
                                   <option value="<%= category._id %>"><%= category.name %></option>
                                    <% }) %> -->
                             </select>
                         
                              <div class="form-text text-muted">select state</div>
                            </div>
                          </div>

                          <script>
                            function getdata(event){
                            //  event.preventDefault()  
                            const name = document.getElementById("selectcountry").value;
                            // console.log();
                            
                            var htmldata="";
                             axios.get("http://xitysiteadmin.profcymadev.in:5000/master/city/data")
                             .then(function(res){
                            
                              res.data.forEach(function(data){
                               
                                if(name===data.countryid){
                                htmldata+=`
                                <option value="${data._id}">${data.name}</option>
                                `;
                                }
                              })
                              
                              document.getElementById("selectstate").innerHTML=htmldata;
                            })
                            }
                              </script>
                          
                          <script src="https://unpkg.com/axios/dist/axios.min.js"></script>
                         
                       
                        
                      </div>
                      <div class="col-md-6 px-5">
                        <!-- <div class="mb-4 row">
                          <label for="image" class="col-md-3 col-sm-4 form-label">Image</label>
                          <div class="col-md-9 col-sm-8">
                            <input type="file" class="form-control" id="image" name="image">
                            <div class="form-text text-muted">Insert Image</div>
                          </div>
                        </div> -->
                        <div class="mb-4 row">
                          <label for="name" class="col-md-3 col-sm-4 form-label">City Name</label>
                          <div class="col-md-9 col-sm-8">
                            <input type="text" class="form-control" id="name" name="name" placeholder="Insert Subategory Name">
                            <div class="form-text text-muted">Insert City Name</div>
                          </div>
                        </div>
                        
                      </div>
                    </div>
                        <div class="row">
                          <div class="col-md-12 text-lg-end text-start my-4 py-3 border-top">
                            
                         <button  class="btn saveBtn my-2 mx-2" type="submit"><i class="fa-solid fa-floppy-disk"></i>&nbsp;Save City</button>
                         <button  class="btn cancelBtn my-2 mx-2" type="reset"><i class="fa-solid fa-arrow-rotate-right"></i>&nbsp;Cancel</button>
   
                          </div>
                        </div>
                      </form>
                  
                    </div>
